#!/bin/bash
#SBATCH --job-name="factorial"
#SBATCH --output="factorial.%j.%N.out"
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=20
#SBATCH --export=ALL
#SBATCH -t 00:04:00
#SBATCH -A sds173

## Environment
 module purge
 module load slurm
 module load cpu
 module load gcc/10.2.0
 module load openmpi/4.0.4


## Use srun to run the job
srun -n 8  ./factorial

